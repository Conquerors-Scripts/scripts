-- FOV Slider
local FOVSlider = Tabs.Fun:AddSlider("FOV", {
    Title = "FOV",
    Description = "Adjust camera Field of View",
    Default = Camera.FieldOfView,
    Min = 10,
    Max = 120,
    Rounding = 1,
    Callback = function(v)
        Camera.FieldOfView = v
    end
})

FOVSlider:OnChanged(function(v)
    Camera.FieldOfView = v
end)

FOVSlider:SetValue(Camera.FieldOfView)


